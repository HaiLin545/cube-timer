<view class="time-box">
    <view wx:if="{{isDNF}}" style="font-size: {{size}}rpx">DNF</view>
    <view wx:else>
        <text class="left" style="font-size: {{size}}rpx;">{{leftPart}}</text>
        <text class="right" style="font-size: {{size*0.75}}rpx;">{{rightPart}}</text>
    </view>
</view>
